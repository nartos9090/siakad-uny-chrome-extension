{
    "name": "Siakad UNY Plugin",
    "version": "3.1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://siakad.uny.ac.id/dashboard"],
            "js": ["config.js", "js/html2canvas.min.js", "js/style.js", "schedule.js"],
            "run_at": "document_end"
        }
    ],
    "host_permissions": ["<all_urls>"],
    "description": "An extension for Siakad UNY. It can show schedule display."
}